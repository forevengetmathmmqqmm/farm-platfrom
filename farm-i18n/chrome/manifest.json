{
  "version": "1.0.3",
  "name": "farm-i18n",
  "manifest_version": 2,
  "description": "farm-i18n 是一个管理 i18n 国际化配置文件的扩展",
  "default_locale": "zh_CN",
  "browser_action": {
    "default_title": "farm-i18n",
    "default_popup": "popup/index.html"
  },
  "icons": {
    "16": "img/icon_54.png",
    "48": "img/icon_144.png"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "background.bundle.js"
    ]
  },
  "permissions": [
    "tabs",
    "storage",
    "<all_urls>",
    "http://*/*",
    "https://*/*"
  ],
  "options_page": "options/index.html",
  "options_ui": {
		"page": "options/index.html",
		"chrome_style": true
	},
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'; ",
  "homepage_url": "https://github.com/icepy"
}
